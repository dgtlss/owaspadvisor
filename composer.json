{
    "name": "dgtlss/owaspadvisor",
    "description": "A Laravel package to help developers implement OWASP Top 10 security guidelines",
    "type": "library",
    "license": "MIT",
    "version": "1.0.2",
    "authors": [
        {
            "name": "Nathan Langer",
            "email": "nathanlanger@googlemail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0|^12.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "spatie/laravel-permission": "^5.0"
    },
    "suggest": {
        "spatie/laravel-permission": "Recommended for role and permission management"
    },
    "autoload": {
        "psr-4": {
            "dgtlss\\OWASPAdvisor\\": "src/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dgtlss\\OWASPAdvisor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "dgtlss\\OWASPAdvisor\\OWASPAdvisorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
} 